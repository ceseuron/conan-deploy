[URL]
Port={{ server_port }}                 ;The Port for your Server

[OnlineSubsystemSteam]
ServerName= "{{ server_name }}"              ;The Name of your Server which is shown in the Server Browser
ServerQueryPort="{{ server_query_port }}"     ;The Port the SteamAPI will query your Server from
ServerPassword="{{ server_password }}"          ;The Password needed to access the Server

[RconPlugin]
RconEnabled = 1
RconPassword = "{{ admin_password }}"
RconPort = "{{ server_rcon_port }}"
RconMaxKarma= 60
